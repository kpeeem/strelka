
@media (min-width: 640px) {
  :global(.headerSticky) {
    position: fixed;
    left: 0;
    right: 0;
    background-color: #2253ff;
    width: 100%;
    background-position: 50%;
    background-size: cover;
    box-sizing: border-box;
    min-height: 80px;
    box-shadow: 0 0 19px 0px rgba(0, 0, 0, 0.58);
    z-index: 1;
    top: 0;
    transition: top 0.2s ease-in-out;
    .content {
      padding-top: 0;
      height: 100px;
    }
    &.slideUp {
      top: -100px;
    }
  }
}


.logo{
    display: block;
    color:transparent;
    font-size:30px;
    width: 130px;
    height: 25px;
    background-size: contain;
    background-repeat: no-repeat;
    background-image: url("data:image/svg+xml;base64,");
}

.content{
  width:1200px;
  margin:0 auto;
  padding-top:20px;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.signUp{
  width: 240px;
  height: 60px;
  background: transparent;
  font-size: 18px;
  color: #FFFFFF;
  letter-spacing: 0;
  border: 2px solid #2253FF;
  font-weight: 400;
  &:hover{
    box-shadow: 0px 0px 6px 0px rgba(34, 83, 255, 0.54);
  }
  cursor:pointer;
}

:global(.headerSticky){
  .signUp{
    box-shadow: 0px 0px 0px 2px rgba(255, 255, 255, 1);
  }
}


.link{
  color:white;
  font-size:16px;
  text-decoration:none;
  font-weight: 400;
  &:hover{
    text-decoration:underline;
  }
}


@media (max-width: 1200px){
  .content{
    width:90vw;
    flex-wrap: wrap;
    flex-direction: column;
    justify-content: space-between;
    height: 250px;
  }
  .logo{
    font-size: 2vw;
    width: 9vw;
  }
  .header{
    margin-bottom: 20px;
  }

}